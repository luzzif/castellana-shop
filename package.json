{
    "name": "gatsby-starter-default",
    "private": true,
    "description": "A simple Gatsby starter",
    "version": "0.1.0",
    "author": "Federico Luzzi <fedeluzzi00@gmail.com>",
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "develop": "rimraf .cache && gatsby develop",
        "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
        "start": "yarn develop",
        "serve": "gatsby serve",
        "lint:code": "eslint \"src/**/*.js\"",
        "lint:prettier": "prettier -l src/**/*.js",
        "lint:commit-message": "commitlint -e",
        "lint:ci": "yarn lint:code && yarn lint:prettier"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.28",
        "@fortawesome/free-brands-svg-icons": "^5.13.0",
        "@fortawesome/free-solid-svg-icons": "^5.13.0",
        "@fortawesome/react-fontawesome": "^0.1.9",
        "gatsby": "^4.25.7",
        "gatsby-image": "^2.3.1",
        "gatsby-plugin-react-helmet": "^3.2.1",
        "gatsby-plugin-sharp": "^2.5.4",
        "gatsby-plugin-sitemap": "^2.3.1",
        "gatsby-plugin-styled-components": "^3.2.1",
        "gatsby-plugin-typography": "^2.4.1",
        "gatsby-source-filesystem": "^2.2.2",
        "gatsby-transformer-sharp": "^2.4.4",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^5.2.1",
        "react-typography": "^0.16.19",
        "reflexbox": "^4.0.6",
        "styled-components": "^5.0.1",
        "typography": "^0.16.19"
    },
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "babel-eslint": "^10.1.0",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "husky": "^4.2.3",
        "prettier": "^2.0.4",
        "rimraf": "^3.0.2"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/luzzif/castellana-shop"
    }
}
